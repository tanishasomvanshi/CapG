using System;
using System.IO;


namespace Lab12Q1
{
    class Program
    {
        static void Main()
        {
            try
            {
                Console.WriteLine("Enter File path");
                string filePath = Console.ReadLine();
                StreamReader sr = new StreamReader(filePath);
                Console.WriteLine(sr.ReadToEnd());
                sr.Close();
            }
            catch(Exception e)
            {
                Console.WriteLine("Unexpected Error Try Again");
            }
        }
    }
}
